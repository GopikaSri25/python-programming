class Solution(object):
    def uncommonFromSentences(self, s1, s2):
        """
        :type s1: str
        :type s2: str
        :rtype: List[str]
        """
        liste=s1.split()+s2.split()
        result=[]
        for i in liste:
            if liste.count(i)==1:
                result.append(i)
        
        return result


        

        
