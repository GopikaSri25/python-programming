class Solution(object):
    def removeDuplicates(self, s):
        a = []
        for x in s:
            if len(a) == 0 :
                a.append(x)
            elif a[len(a)-1]!=x:
                a.append(x)
            elif a[len(a)-1]==x:
                a.pop()
        return "".join(a)

            




        
