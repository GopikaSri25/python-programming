class Solution(object):
    def specialArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """

        for i in range(1,max(nums)+1):
            count=0
            for j in nums:
                if i<=j:
                    count+=1
            
            if count==i:
                return i

        return -1
             
                
        

            
        
