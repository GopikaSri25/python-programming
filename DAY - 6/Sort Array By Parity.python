class Solution(object):
    def sortArrayByParity(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        arr = []
        for i in range(len(nums)):
            if nums[i] % 2 == 0:
                arr.append(nums[i])
        for i in range(len(nums)):
            if nums[i] % 2 == 1:
                arr.append(nums[i])

        return arr
        
